{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "plugin:react/recommended",
    "google"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "react",
    "@typescript-eslint"
  ],
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "rules": {
    "object-curly-spacing": ["error", "always"],
    "indent": ["error", 2, { "SwitchCase": 1 }],
    "quote-props": ["error", "as-needed"],
    "max-len": ["error", {"code": 210}],
    "require-jsdoc": "off",
    "no-invalid-this": "off",
    "linebreak-style": "off",
    "react/prop-types": 0,
    "no-trailing-spaces": "off",
    "operator-linebreak": ["error", "before"],
    "semi": ["error", "never"],
    "@typescript-eslint/no-unused-vars": "error"
  }
}
